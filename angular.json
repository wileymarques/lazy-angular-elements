{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "lazy-angular-elements": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/lazy-angular-elements",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [
              {
                "input": "node_modules/document-register-element/build/document-register-element.js"
              }
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "lazy-angular-elements:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "lazy-angular-elements:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "lazy-angular-elements:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "lazy-loaded-element": {
      "root": "projects/lazy-loaded-element",
      "sourceRoot": "projects/lazy-loaded-element/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/lazy-loaded-element/tsconfig.lib.json",
            "project": "projects/lazy-loaded-element/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/lazy-loaded-element/src/test.ts",
            "tsConfig": "projects/lazy-loaded-element/tsconfig.spec.json",
            "karmaConfig": "projects/lazy-loaded-element/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/lazy-loaded-element/tsconfig.lib.json",
              "projects/lazy-loaded-element/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "data-handler": {
      "root": "projects/data-handler",
      "sourceRoot": "projects/data-handler/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/data-handler/tsconfig.lib.json",
            "project": "projects/data-handler/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/data-handler/src/test.ts",
            "tsConfig": "projects/data-handler/tsconfig.spec.json",
            "karmaConfig": "projects/data-handler/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/data-handler/tsconfig.lib.json",
              "projects/data-handler/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "lazy-loaded": {
      "root": "projects/lazy-loaded/",
      "sourceRoot": "projects/lazy-loaded/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/lazy-loaded",
            "index": "projects/lazy-loaded/src/index.html",
            "main": "projects/lazy-loaded/src/main.ts",
            "polyfills": "projects/lazy-loaded/src/polyfills.ts",
            "tsConfig": "projects/lazy-loaded/tsconfig.app.json",
            "assets": [
              "projects/lazy-loaded/src/favicon.ico",
              "projects/lazy-loaded/src/assets"
            ],
            "styles": [
              "projects/lazy-loaded/src/styles.css"
            ],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/lazy-loaded/src/environments/environment.ts",
                  "with": "projects/lazy-loaded/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "lazy-loaded:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "lazy-loaded:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "lazy-loaded:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/lazy-loaded/src/test.ts",
            "polyfills": "projects/lazy-loaded/src/polyfills.ts",
            "tsConfig": "projects/lazy-loaded/tsconfig.spec.json",
            "karmaConfig": "projects/lazy-loaded/karma.conf.js",
            "styles": [
              "projects/lazy-loaded/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "projects/lazy-loaded/src/favicon.ico",
              "projects/lazy-loaded/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/lazy-loaded/tsconfig.app.json",
              "projects/lazy-loaded/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "lazy-loaded-e2e": {
      "root": "projects/lazy-loaded-e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/lazy-loaded-e2e/protractor.conf.js",
            "devServerTarget": "lazy-loaded:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "lazy-loaded:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "projects/lazy-loaded-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "lazy-angular-elements"
}
